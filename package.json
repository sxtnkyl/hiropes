    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
